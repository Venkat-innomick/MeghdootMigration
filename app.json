{
  "expo": {
    "name": "AGROMET",
    "slug": "agromet-rn-eas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon.png",
    "scheme": "meghdoot-app",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "backgroundColor": "#749C53",
      "barStyle": "light-content",
      "translucent": false
    },
    "androidNavigationBar": {
      "backgroundColor": "#749C53",
      "barStyle": "light-content"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.agromet.app"
    },
    "android": {
      "package": "com.aas.meghdoot",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon.png",
        "backgroundColor": "#749C53"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-location",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow AGROMET to access your photos.",
          "cameraPermission": "Allow AGROMET to access your camera."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/AppIcon.png",
          "color": "#749C53"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "replace-with-your-eas-project-id"
      },
      "apiBaseUrl": "http://103.215.208.11"
    }
  }
}
